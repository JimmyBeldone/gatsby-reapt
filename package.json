{
  "name": "gatsby-starter-reapt",
  "private": true,
  "description": "A Gatsby version of Reapt with Redux, i18n, Stylus and more",
  "version": "0.0.0-development",
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "keywords": [
    "gatsby",
    "react",
    "boilerplate",
    "starterkit",
    "starter kit",
    "webpack",
    "stylus",
    "redux",
    "i18n",
    "code formatter",
    "mk-react-comp",
    "pwa",
    "progressive web app"
  ],
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "setup": "node setup/setupMessage.js && yarn && node setup/setup.js",
    "develop": "cross-env NODE_ENV=development MODE=development gatsby develop -o",
    "start": "yarn develop",
    "build": "cross-env NODE_ENV=production MODE=production gatsby build",
    "build:staging": "cross-env NODE_ENV=production MODE=staging gatsby build",
    "start:mobile": "yarn develop -H 0.0.0.0",
    "serve": "gatsby serve -o",
    "serveMobile": "gatsby serve -o -H 0.0.0.0",
    "optim-img": "node scripts/optimize.js",
    "https": "http-server public -o -g --ssl --cert ~/.localhost-ssl/test.crt --key ~/.localhost-ssl/test.key",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "stylint": "stylint",
    "lint": "eslint src --fix",
    "pretty": "prettier --write 'src/**/*.js'",
    "clean": "rimraf .cache public",
    "mrc": "node ./node_modules/mk-react-comp/lib/index.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-reapt"
  },
  "bugs": {
    "url": "https://github.com/JimmyBeldone/gatsby-reapt/issues"
  },
  "dependencies": {
    "clsx": "1.0.4",
    "date-fns": "^1.30.1",
    "gatsby": "2.9.4",
    "gatsby-image": "2.1.4",
    "gatsby-plugin-advanced-sitemap": "1.0.6",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-offline": "2.1.3",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.2.0",
    "gatsby-plugin-robots-txt": "1.4.0",
    "gatsby-plugin-sharp": "2.1.5",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-source-filesystem": "2.0.39",
    "gatsby-transformer-remark": "2.4.0",
    "gatsby-transformer-sharp": "2.1.21",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-intl": "2.9.0",
    "react-redux": "7.1.0",
    "react-typography": "0.16.19",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "typeface-lato": "0.0.54",
    "typeface-montserrat": "0.0.54",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "chalk": "2.4.2",
    "commitizen": "^3.1.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-security": "1.4.0",
    "gatsby-plugin-eslint": "2.0.5",
    "gatsby-plugin-stylus-resources": "1.0.13",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "mk-react-comp": "^1.1.7",
    "prettier": "^1.18.2",
    "prompt": "1.0.0",
    "replace": "1.1.0",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.16",
    "stylint": "1.5.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.(js|jsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.(styl|css)": [
      "stylint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
