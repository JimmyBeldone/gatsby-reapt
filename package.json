{
  "name": "gatsby-starter-reapt",
  "private": true,
  "description": "A Gatsby version of Reapt with Redux, i18n, Stylus and more",
  "version": "3.4.0",
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "keywords": [
    "gatsby",
    "react",
    "boilerplate",
    "starterkit",
    "starter kit",
    "webpack",
    "stylus",
    "i18n",
    "code formatter",
    "mk-react-comp",
    "pwa",
    "progressive web app"
  ],
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "setup": "node setup/setupMessage.js && yarn && node setup/setup.js",
    "develop": "cross-env NODE_ENV=development MODE=development gatsby develop -o",
    "start": "yarn develop",
    "build": "cross-env NODE_ENV=production MODE=production gatsby build",
    "build:staging": "cross-env NODE_ENV=production MODE=staging gatsby build",
    "start:mobile": "yarn develop -H 0.0.0.0",
    "serve": "gatsby serve -o",
    "serveMobile": "gatsby serve -o -H 0.0.0.0",
    "optim-img": "node scripts/imageOptimizer.js",
    "https": "http-server public -o -g --ssl --cert ~/.localhost-ssl/test.crt --key ~/.localhost-ssl/test.key",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "stylint": "stylint",
    "lint": "eslint src --fix",
    "pretty": "prettier --write 'src/**/*.js'",
    "clean": "rimraf .cache public",
    "mrc": "node ./node_modules/mk-react-comp/lib/index.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-reapt"
  },
  "bugs": {
    "url": "https://github.com/JimmyBeldone/gatsby-reapt/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.5.7",
    "@mdx-js/react": "1.5.7",
    "babel-plugin-styled-components": "1.10.7",
    "clsx": "1.1.0",
    "date-fns": "2.10.0",
    "gatsby": "2.19.32",
    "gatsby-image": "2.2.43",
    "gatsby-plugin-lunr": "1.5.2",
    "gatsby-plugin-manifest": "2.2.44",
    "gatsby-plugin-mdx": "1.0.77",
    "gatsby-plugin-netlify": "2.1.34",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "2.1.20",
    "gatsby-plugin-offline": "3.0.37",
    "gatsby-plugin-react-helmet-async": "1.0.16",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.4.7",
    "gatsby-plugin-sitemap": "2.2.28",
    "gatsby-plugin-styled-components": "3.1.20",
    "gatsby-plugin-typography": "2.3.23",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.46",
    "gatsby-schema-field-absolute-path": "1.1.1",
    "gatsby-source-filesystem": "2.1.50",
    "gatsby-transformer-sharp": "2.3.17",
    "grommet": "2.11.1",
    "intersection-observer": "^0.7.0",
    "lodash.kebabcase": "4.1.1",
    "lodash.orderby": "4.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-headroom": "3.0.0",
    "react-helmet-async": "1.0.4",
    "react-intl": "4.1.1",
    "react-share": "4.0.1",
    "react-typography": "0.16.19",
    "styled-components": "5.0.1",
    "typeface-lato": "0.0.75",
    "typeface-montserrat": "0.0.75",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "babel-eslint": "10.1.0",
    "chalk": "3.0.0",
    "commitizen": "4.0.3",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-universe": "2.3.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-stylus-resources": "1.0.25",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.8",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mk-react-comp": "1.1.8",
    "prettier": "1.19.1",
    "prompt": "1.0.0",
    "replace": "1.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.4",
    "stylint": "2.0.0",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.(js|jsx)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.(styl|css)": [
      "stylint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
